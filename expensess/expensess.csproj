    </EmbeddedResource>
    <EmbeddedResource Update="Page\ForgotPasswordPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Page\Singup.xaml">
